#include <iostream>
#include<string>
using namespace std;

struct CandyBar
{
	string name;
	double ves;
	int kalorii;
};

int main() {
	CandyBar snack = { "Mocha Munch", 350, 2.3 };
	cout << "Name: " << snack.name<<endl;
	cout << "ves: " << snack.ves<<endl;
	cout << "Kalorii: " << snack.kalorii;
		return 0;
}
